{
    "env": {
        "browser": true,
        "jest": true,
        "node": true
    },
    "parser": "babel-eslint",
    "rules": {
        "indent": 0,
        "quotes": [2, "double", { "avoidEscape": true }],
        "linebreak-style": 0
        },
    "settings": {
        "import/resolver": {
            "node": {
                "paths": [
                    "src"
                ]
            }
        }
    }
}